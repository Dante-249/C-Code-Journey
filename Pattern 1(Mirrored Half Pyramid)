#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int i,j;
    int a=10;
    cout<<"hello world!"<<endl;
    for(i=1;i<=a;i++)
    {
        for(j=1;j<=a;j++)
       {
    if(i+j<=a)
        cout<<" ";
        else
        cout<<"*";
       }
        cout<<endl;
    }
   
   

    return 0;
}
