// Online C++ compiler to run C++ program online
// Inverted Half Pyramid Pattern
#include <iostream>
using namespace std;
int main() {
    
    int i,j;
    int a=10;
    cout<<"hello world!"<<endl;
    for(i=1;i<=a;i++)
    {
        for(j=a;j>=i;j--)
       {
        cout<<"*";
       }
        cout<<endl;
    }
    return 0;
}
