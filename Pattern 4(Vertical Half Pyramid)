#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    
    int i,j,l;
    int a=5;
    int b=3;
 for(l=1;l<=b;l++)   
{
   for(i=1;i<=a;i++)
   {
   {
       for(j=1;j<=a;j++)
       if(j<=i)
       cout<<"*";
       else
       cout<<" ";
   }
        cout<<endl;
   }
}
